{
  "expo": {
    "name": "DietWithBrandon",
    "slug": "DietWithBrandon",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "dietwithbrandon",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dietwithbrandon"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dietwithbrandon"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ],
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "8e2502b9-e20e-48c0-aa43-07b983d5e6e1"
      }
    }
  }
}
